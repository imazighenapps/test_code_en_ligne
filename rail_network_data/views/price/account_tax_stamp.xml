<odoo>
  <data>



    <record model="ir.ui.view" id="account_tax_stamp_form">
      <field name="name">account.tax.stamp.form</field>
      <field name="model">account.tax.stamp</field>
      <field name="arch" type="xml">
        <form string="Timbre fiscal">
          <sheet>
           <group>
              <group>
                  <field name="name"/>
              </group>
           </group>
            <notebook>
             <page string="Configuration de calcul de timbre">
                  <field name="stamp_line_ids">
                      <list editable="bottom">
                          <field name="from_dinar" width="20%"/>        
                          <field name="to_dinar" width="20%"/>           
                          <field name="calculation_type" width="20%"/>    
                          <field name="amount" width="20%"/>  
                      </list>
                  </field>
              </page>
            </notebook>

          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="account_tax_stamp_list">
      <field name="name">account.tax.stamp.list</field>
      <field name="model">account.tax.stamp</field>
      <field name="arch" type="xml">
        <list string="Timbre fiscal">
            <field name="name"/>
        </list>
      </field>
    </record>

    <record model="ir.actions.act_window" id="account_tax_stamp_action">
      <field name="name">Timbre fiscal</field>
      <field name="res_model">account.tax.stamp</field>
      <field name="view_mode">list,form</field>
      <field name="target">current</field>
    </record>
  </data>
</odoo>